{
  "name": "emme-interface",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/data-grid": "^4.0.0-alpha.11",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/lodash.flatmap": "^4.5.6",
    "@types/node": "^14.14.28",
    "@types/react": "^17.0.2",
    "@types/react-anchor-link-smooth-scroll": "^1.0.1",
    "@types/react-copy-to-clipboard": "^5.0.1",
    "@types/react-dom": "^17.0.1",
    "axios": "^0.21.1",
    "classnames": "^2.2.6",
    "clsx": "^1.1.1",
    "country-state-city": "^3.0.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "formik": "^2.2.9",
    "hamburger-react": "^2.4.1",
    "i18next": "^19.8.9",
    "i18next-browser-languagedetector": "^6.0.1",
    "i18next-xhr-backend": "^3.2.2",
    "lodash": "^4.17.20",
    "lodash.flatmap": "^4.5.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-anchor-link-smooth-scroll": "^1.0.12",
    "react-copy-to-clipboard": "^5.0.4",
    "react-dom": "^17.0.1",
    "react-i18next": "^11.8.7",
    "react-icons": "^4.2.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.2",
    "redux-localstorage-simple": "^2.3.1",
    "typescript": "^4.1.5",
    "uuid-apikey": "^1.5.3",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts --max_old_space_size=4096 start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "lint": "eslint src",
    "deploy:prod": "yarn build && tar -cvf ./deploy.tar --exclude='*.map' ./captain-definition ./build/* && caprover deploy -t ./deploy.tar -n captain-01 -a prod",
    "deploy:dev": "yarn build && tar -cvf ./deploy.tar --exclude='*.map' ./captain-definition ./build/* && caprover deploy -t ./deploy.tar -n captain-01 -a dev"
  },
  "lint-staged": {
    "*.{ts,tsx,json}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "settings": {
      "import/resolver": {
        "typescript": {}
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/lodash": "^4.14.168",
    "@types/qs": "^6.9.6",
    "@types/react-redux": "^7.1.11",
    "@types/react-router-dom": "^5.1.6",
    "@types/utf8": "^2.1.6",
    "@types/uuid": "^8.3.1",
    "cross-env": "^7.0.3",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "pretty-quick": "^3.1.0"
  }
}
